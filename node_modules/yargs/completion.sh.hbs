###-begin-{{app_name***REMOVED******REMOVED***-completions-###
#
# yargs command completion script
#
# Installation: {{app_path***REMOVED******REMOVED*** completion >> ~/.bashrc
#    or {{app_path***REMOVED******REMOVED*** completion >> ~/.bash_profile on OSX.
#
_yargs_completions()
{
    local cur_word args type_list

    cur_word="${COMP_WORDS[COMP_CWORD]***REMOVED***"
    args=$(printf "%s " "${COMP_WORDS[@]***REMOVED***")

    # ask yargs to generate completions.
    type_list=`{{app_path***REMOVED******REMOVED*** --get-yargs-completions $args`

    COMPREPLY=( $(compgen -W "${type_list***REMOVED***" -- ${cur_word***REMOVED***) )

    # if no match was found, fall back to filename completion
    if [ ${#COMPREPLY[@]***REMOVED*** -eq 0 ]; then
      COMPREPLY=( $(compgen -f -- "${cur_word***REMOVED***" ) )
    fi

    return 0
***REMOVED***
complete -F _yargs_completions {{app_name***REMOVED******REMOVED***
###-end-{{app_name***REMOVED******REMOVED***-completions-###
